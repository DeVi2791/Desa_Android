<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nextu.dvillamar.manifest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Inicio"
            android:label="@string/inicio" />
        <activity android:name=".Tv"
            android:label="@string/tv"/>
        <activity android:name=".Smartphone"
            android:label="@string/smartp"/>
        <activity android:name=".Tablet"
            android:label="@string/tablet"></activity>
    </application>

</manifest>